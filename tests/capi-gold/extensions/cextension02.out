<?xml version="1.0" encoding="UTF-8"?>
<out xmlns:xalan="http://xml.apache.org/xalan">

*****				DATA				*****
ns0: /lists/*/*
item=A,item=B,item=C,item=D,item=E,item=A,item=B,item=C,item=D,item=E,item=A,item=B,item=C,item=X,item=Y,item=Z,item=X,item=Y,item=Z,item=A,item=B,item=C,item=D,item=E,item=A,item=B,item=C,item=D,item=E,item=X,item=Y,item=Z,item=X,item=Y,item=Z,item=A,item=B,item=C,item=D,item=E,

ns3: /lists/list3/*
item=X,item=Y,item=Z,

ns4: /lists/*/item
item=A,item=B,item=C,item=D,item=E,item=A,item=B,item=C,item=D,item=E,item=A,item=B,item=C,item=X,item=Y,item=Z,item=X,item=Y,item=Z,item=A,item=B,item=C,item=D,item=E,item=A,item=B,item=C,item=D,item=E,item=X,item=Y,item=Z,item=X,item=Y,item=Z,item=A,item=B,item=C,item=D,item=E,

ns5: /lists/list5/item
